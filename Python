import requests
from bs4 import BeautifulSoup

def extract_display_text(url):
    headers = {
        'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 '
                      '(KHTML, like Gecko) Chrome/122.0.0.0 Safari/537.36'
    }

    try:
        response = requests.get(url, headers=headers)
        response.raise_for_status()
    except Exception as e:
        print(f"Failed to fetch URL: {e}")
        return []

    soup = BeautifulSoup(response.text, 'html.parser')
    result = []

    for class_name in ['Display1', 'Display2']:
        divs = soup.find_all('div', class_=class_name)
        for div in divs:
            text = div.get_text(strip=True)
            if text:
                result.append((class_name, text))

    return result

# Example usage
url = "https://example.com"  # Replace with your real URL
texts = extract_display_text(url)

for class_name, text in texts:
    print(f"[{class_name}] {text}")
